<link href="~/vendors/Javascripts/jquery-jvectormap/jquery-jvectormap-1.2.2.css" rel="stylesheet" />

<div style="margin-top: 60px; margin-left: auto; margin-right: auto; width: 600px;">
    <h2>Test af kort</h2>

    <div style="width:600px; height: 600px">
        <div id="map" style="width:100%; height:100%">

        </div>
    </div>
</div>
<script src="~/vendors/Javascripts/jquery-2.1.0.min.js"></script>
<script src="~/vendors/Javascripts/jquery-jvectormap/jquery-jvectormap-1.2.2.min.js"></script>
<script src="~/vendors/Javascripts/jquery-jvectormap/jquery-jvectormap-dk-merc-en.js"></script>

<script>

    $(function () {
        $('#map').vectorMap({
            map: 'dk_merc_en',
            backgroundColor: "#fff",
            regionStyle: {
                initial: {
                    fill: '#666',
                    "fill-opacity": 1,
                    stroke: 'none',
                    "stroke-width": 0,
                    "stroke-opacity": 1
                },
                hover: {
                },
                selected: {
                    fill: 'yellow'
                },
                selectedHover: {
                }
            },
            zoomOnScroll: false
        });
    });

</script>

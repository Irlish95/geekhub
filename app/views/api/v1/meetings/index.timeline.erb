{
    "timeline":
    {
        "headline":"Events i det danske udvikler community",
        "type":"default",
		"startDate": <%= raw(Time.now.to_json)%>,
		"text":"Powered by geekhub.dk",
        "date": [
        <%= raw(@meetings.map do |m|
            {
                startDate: m.starts_at,
                headline: m.title,
                text: timeline_text(m)
            }.to_json
        end.join(',')) %>
        ]
    }
}
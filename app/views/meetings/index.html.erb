<% @meetings.each do |meeting| %>
  <div>
    <h2><%= meeting.title %></h2>
    <div><%= meeting.location %> - <%= meeting.starts_at %> - <%= meeting.organizer %></div>
    <div class="details" style="display:none;">
      <div><%= meeting.description %></div>
      <div><%= meeting.url %></div>
    </div>
    <div><%= link_to 'Edit', edit_meeting_path(meeting) %></div>
  </div>
<% end %>

<%= link_to 'New Meeting', new_meeting_path %>

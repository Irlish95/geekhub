<%= RiCal.Calendar do |cal|
  @meetings.each do |m|
    cal.event do |e|
      e.summary m.title
      e.dtstart     m.starts_at
      e.dtend       (m.starts_at + 3.hours)
      e.location    m.city.name
      e.description m.description + "\n\nLæs mere: " + m.url
      e.url         m.url
      e.alarm do
        description m.title
      end
    end
  end
end.export %>
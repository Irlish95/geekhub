<div class="tags">
  <div class="tags-tag location"><%= meeting.city.try(:name) %></div>
    <% unless meeting.costs_money %>
      <div class="tags-tag free">Gratis</div>
    <% end %>
  <div class="tags-tag organizer"><%= meeting.organizer %></div>
  <% if meeting.joinable? %>
    <div class="tags-tag attendees">
      <div class="image">
        <%= image_tag "/person.png" %>
      </div><%= pluralize meeting.attendee_count, 'deltager', 'deltagere' %>
    </div>
  <% end %>
</div>